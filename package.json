{
  "name": "chaant",
  "description": "ChAAnt (Chess Auto Agent) is a humble attempt to create a middleman agent for playing chess in various website by utilizing puppeteer.",
  "workspaces": [
    "chaants/*",
    "chaant/core"
  ],
  "scripts": {
    "sample-console": "cd chaants/chaant-core && npm run sample-console",
    "test": "cd chaants/chaant-core && npm run test",
    "agent-api": "cd chaants/chaant-core && npm run agent-api",
    "chaant-desktop": "cd chaants/chaant-desktop && npm run start",
    "build": "tsc --build --verbose tsconfig.build.json",
    "desktop-make": "cd chaants/chaant-desktop && npm run make"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/node": "^20.6.0",
    "@types/jest": "^29.5.12",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "tsconfig-paths": "^4.2.0"
  }
}
